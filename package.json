{
  "name": "iconic-gestione-staff",
  "version": "1.0.1",
  "description": "Dashboard Node.js/Express/EJS per gestione staff ICONIC",
  "main": "server.js",
  "scripts": {
    "start": "node --no-warnings server.js",
    "dev": "nodemon --no-warnings server.js",
    "postinstall": "npm rebuild sqlite3",
    "railway-init": "node setup-volume.js && node railway-init.js",
    "init-tables": "node setup-volume.js && node create-missing-tables.js",
    "test-db": "node test-database.js",
    "clear-db": "node setup-volume.js && node clear-database.js --confirm",
    "clear-db-safe": "node setup-volume.js && node clear-database.js"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "connect-flash": "^0.1.1",
    "connect-session-sequelize": "^8.0.2",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.9",
    "express": "^4.19.2",
    "express-ejs-layouts": "^2.5.1",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.18.2",
    "express-validator": "^7.3.0",
    "helmet": "^8.1.0",
    "serve-favicon": "^2.5.1",
    "sqlite3": "^5.1.6",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "ejs-lint": "^2.0.1",
    "nodemon": "^3.1.0"
  }
}
