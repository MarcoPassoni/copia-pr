      <script>
        // Animazioni al caricamento
        document.addEventListener('DOMContentLoaded', function() {
          console.log('Pagina Guadagni con dati reali caricata');
          console.log('Totali:', {
            fatturato: '<%= totali.fatturatoTotale %>',
            guadagnoNetto: '<%= totali.guadagnoNettoTotale %>',
            numeroPR: '<%= totali.numeroPR %>'
          });
          
          // Anima le cards statistiche
          const statCards = document.querySelectorAll('.stat-card');
          statCards.forEach((card, index) => {
            setTimeout(() => {
              card.style.opacity = '0';
              card.style.transform = 'translateY(20px)';
              card.style.transition = 'all 0.6s ease';
              setTimeout(() => {
                card.style.opacity = '1';
                card.style.transform = 'translateY(0)';
              }, 100);
            }, index * 100);
          });
          
          // Anima le righe della tabella
          const tableRows = document.querySelectorAll('.pr-row');
          tableRows.forEach((row, index) => {
            setTimeout(() => {
              row.style.opacity = '0';
              row.style.transform = 'translateX(-20px)';
              row.style.transition = 'all 0.5s ease';
              setTimeout(() => {
                row.style.opacity = '1';
                row.style.transform = 'translateX(0)';
              }, 100);
            }, (index + 1) * 150);
          });
          
          // Tooltip per le colonne
          const tooltips = {
            'Fatturato': 'Totale incassi generati dal PR',
            'Guadagno Lordo': 'Ricavo admin prima delle detrazioni',
            'Passo': 'Quota destinata a Passo (20%)',
            'Cassa': 'Quota destinata a Cassa (15%)', 
            'Cuzzo': 'Quota destinata a Cuzzo (15%)',
            'Guadagno Netto': 'Ricavo effettivo admin'
          };
          
          // Evidenzia totali in footer
          const footerCells = document.querySelectorAll('tfoot th');
          footerCells.forEach(cell => {
            cell.addEventListener('mouseenter', function() {
              this.style.transform = 'scale(1.05)';
              this.style.transition = 'transform 0.2s ease';
            });
            
            cell.addEventListener('mouseleave', function() {
              this.style.transform = 'scale(1)';
            });
          });
          
          // Effetto hover migliorato per le righe
          tableRows.forEach(row => {
            row.addEventListener('mouseenter', function() {
              this.style.backgroundColor = '#f8f9fa';
              this.style.boxShadow = '0 4px 15px rgba(0,0,0,0.1)';
              this.style.transform = 'scale(1.02)';
              this.style.transition = 'all 0.3s ease';
            });
            
            row.addEventListener('mouseleave', function() {
              this.style.backgroundColor = '';
              this.style.boxShadow = '';
              this.style.transform = 'scale(1)';
            });
          });
          
          // Formattazione numeri con separatori migliaia
          const formatNumber = (num) => {
            return new Intl.NumberFormat('it-IT', {
              style: 'currency',
              currency: 'EUR',
              minimumFractionDigits: 2,
              maximumFractionDigits: 2
            }).format(num);
          };
          
          // Anima le progress bar
          const progressBars = document.querySelectorAll('.progress-bar');
          setTimeout(() => {
            progressBars.forEach(bar => {
              const width = bar.style.width;
              bar.style.width = '0%';
              bar.style.transition = 'width 1.5s ease-in-out';
              setTimeout(() => {
                bar.style.width = width;
              }, 200);
            });
          }, 1000);
          
          // Aggiungi icone dinamiche ai valori negativi
          const negativeCells = document.querySelectorAll('.text-loss');
          negativeCells.forEach(cell => {
            if (!cell.querySelector('i')) {
              const icon = document.createElement('i');
              icon.className = 'fas fa-arrow-down me-1';
              icon.style.color = '#e74c3c';
              cell.prepend(icon);
            }
          });
          
          // Aggiungi icone dinamiche ai valori positivi
          const positiveCells = document.querySelectorAll('.text-profit');
          positiveCells.forEach(cell => {
            if (!cell.querySelector('i')) {
              const icon = document.createElement('i');
              icon.className = 'fas fa-arrow-up me-1';
              icon.style.color = '#27ae60';
              cell.prepend(icon);
            }
          });
        });
        
        // Funzione per aggiornare i dati
        function aggiornaDati() {
          const btn = event.target;
          const icon = btn.querySelector('i');
          
          // Mostra loading
          btn.disabled = true;
          icon.className = 'fas fa-spinner fa-spin';
          btn.innerHTML = '<i class="fas fa-spinner fa-spin"></i> Aggiornando...';
          
          // Simula aggiornamento e ricarica la pagina
          setTimeout(() => {
            location.reload();
          }, 1000);
        }
        
        // Funzione per esportare i dati in formato CSV
        function esportaDati() {
          const btn = event.target;
          const icon = btn.querySelector('i');
          
          try {
            // Mostra loading
            btn.disabled = true;
            const originalText = btn.innerHTML;
            btn.innerHTML = '<i class="fas fa-spinner fa-spin"></i> Esportando...';
            
            // Raccogli i dati dalla tabella
            const dataEsportazione = [
              ['PR', 'Percentuale Provvigione', 'Fatturato', 'Eventi', 'Guadagno Lordo', 'Passo (20%)', 'Cassa (15%)', 'Cuzzo (15%)', 'Guadagno Netto']
            ];
            
            // Estrai dati dalla tabella PR se presente
            const tabellaCorpo = document.querySelector('.table-responsive tbody');
            if (tabellaCorpo) {
              const righe = tabellaCorpo.querySelectorAll('tr');
              righe.forEach((riga, index) => {
                const celle = riga.querySelectorAll('td');
                if (celle.length >= 8) {
                  // Estrai il testo dalle celle rimuovendo elementi HTML
                  const riga_dati = [];
                  celle.forEach((cella, cellIndex) => {
                    if (cellIndex === 0) {
                      // Prima cella contiene nome + dati personali
                      const nickname = cella.querySelector('.fw-bold')?.textContent?.trim() || '';
                      const nome = cella.querySelector('.text-muted')?.textContent?.trim() || '';
                      riga_dati.push(nickname + (nome ? ' (' + nome + ')' : ''));
                    } else {
                      riga_dati.push(cella.textContent.trim());
                    }
                  });
                  dataEsportazione.push(riga_dati);
                }
              });
            }
            
            // Aggiungi dati riassuntivi dalle statistiche
            const statCards = document.querySelectorAll('.stat-card .stat-value');
            if (statCards.length >= 4) {
              dataEsportazione.push(['']); // Riga vuota
              dataEsportazione.push(['STATISTICHE GENERALI']);
              dataEsportazione.push(['Fatturato Totale', statCards[0].textContent.trim()]);
              dataEsportazione.push(['Eventi Totali', statCards[1].textContent.trim()]);
              dataEsportazione.push(['Guadagno Lordo', statCards[2].textContent.trim()]);
              dataEsportazione.push(['Guadagno Netto', statCards[3].textContent.trim()]);
            }
            
            // Converti in CSV
            const csvContent = dataEsportazione.map(row => 
              row.map(cell => `"${cell}"`).join(',')
            ).join('\n');
            
            // Crea e scarica il file
            const blob = new Blob(['\ufeff' + csvContent], { type: 'text/csv;charset=utf-8;' });
            const link = document.createElement('a');
            
            if (link.download !== undefined) {
              const url = URL.createObjectURL(blob);
              link.setAttribute('href', url);
              
              // Nome file con data
              const oggi = new Date();
              const dataFormattata = oggi.toISOString().split('T')[0];
              link.setAttribute('download', `guadagni_admin_${dataFormattata}.csv`);
              
              link.style.visibility = 'hidden';
              document.body.appendChild(link);
              link.click();
              document.body.removeChild(link);
              
              // Mostra notifica di successo
              setTimeout(() => {
                alert('✅ Report esportato con successo!');
              }, 500);
            } else {
              throw new Error('Download non supportato dal browser');
            }
            
          } catch (error) {
            console.error('Errore esportazione:', error);
            alert('❌ Errore durante l\'esportazione del report');
          } finally {
            // Ripristina pulsante
            setTimeout(() => {
              btn.disabled = false;
              btn.innerHTML = '<i class="fas fa-download"></i> Esporta';
            }, 1500);
          }
        }
        
        // Funzione per gestire staff (reindirizza alla pagina staff)
        function gestisciStaff() {
          window.location.href = '/admin/staff';
        }
        
        // Funzione per aggiornare dati (alias per compatibilità)
        function refreshData() {
          aggiornaDati();
        }
        
        // Funzione per esportare dati (alias per compatibilità)
        function exportData() {
          esportaDati();
        }
      </script>